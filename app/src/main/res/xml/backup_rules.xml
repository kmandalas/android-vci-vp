<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude EncryptedSharedPreferences - keys don't transfer between devices -->
    <exclude domain="sharedpref" path="wallet_prefs.xml"/>
    <!-- Also exclude the Tink keyset preferences -->
    <exclude domain="sharedpref" path="__androidx_security_crypto_encrypted_prefs__.xml"/>
</full-backup-content>
